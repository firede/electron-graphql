{
  "name": "electron-graphql",
  "version": "0.1.0",
  "description": "",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "scripts": {
    "prepublish": "npm run dist",
    "precommit": "lint-staged",
    "prepush": "npm test",
    "dist:clean": "rimraf dist",
    "dist:build": "tsc",
    "dist": "npm run dist:clean && npm run dist:build",
    "test": "jest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/firede/electron-graphql.git"
  },
  "keywords": [
    "graphql",
    "electron",
    "fetch"
  ],
  "author": "Firede <firede@firede.us>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/firede/electron-graphql/issues"
  },
  "homepage": "https://github.com/firede/electron-graphql#readme",
  "devDependencies": {
    "@types/jest": "^22.0.1",
    "@types/node": "^9.3.0",
    "electron": "^1.8.1",
    "graphql": "^0.12.3",
    "husky": "^0.14.3",
    "jest": "^22.1.1",
    "lint-staged": "^6.0.0",
    "prettier": "^1.10.2",
    "rimraf": "^2.6.2",
    "spectron": "^3.7.2",
    "ts-jest": "^22.0.1",
    "typescript": "^2.6.2"
  },
  "peerDependencies": {
    "electron": "^1.7.0 || ^1.8.0",
    "graphql": "^0.9.0 || ^0.10.0 || ^0.11.0 || ^0.12.0"
  },
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "prettier --write",
      "git add"
    ]
  }
}
